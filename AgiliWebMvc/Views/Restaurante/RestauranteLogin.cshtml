@model AgiliWebMvc.Models.Restaurantes
@{
    ViewData["Title"] = "RestauranteLogin";
}

<h2>Login</h2>


<div class="row">
    <div class="col-md-4">
        <form asp-action="RestauranteLogin">
            <div class="form-group">
                <label asp-for="cnpj_rest" class="control-label">CNPJ</label>
                <input asp-for="cnpj_rest" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="senha_rest" class="control-label">SENHA</label>
                <input asp-for="senha_rest" class="form-control" type="password" />
            </div>
            <div class="form-group">
                <input type="submit" value="Entrar" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
